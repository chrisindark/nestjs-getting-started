{
  "name": "@christopherpaul/nestjs-getting-started",
  "version": "0.0.1",
  "description": "",
  "author": "Christopher Paul <chrisindark.notafraid@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:dev:pubsub": "NODE_ENV=development nest start --watch --config pubsub-nest-cli.json",
    "start:dev:cron": "NODE_ENV=development nest start --watch --config cron-nest-cli.json",
    "start:dev:cl": "NODE_ENV=development nest start --config cl-nest-cli.json",
    "start:dev:websocket": "NODE_ENV=development nest start --watch --config websocket-nest-cli.json",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=prod node dist/main",
    "pm2:prod": "pm2 reload ecosystem.config.js --env production",
    "pm2:dev": "pm2 reload ecosystem.config.js --env development",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "compile": "tsc --noEmit -p .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/nestjs-getting-started/test/jest-e2e.json",
    "pm2-start:prod": "pm2 start ecosystem.config.js --env prod",
    "pm2-stop:prod": "pm2 stop",
    "pm2-list:prod": "pm2 list",
    "pm2-logs:prod": "pm2 logs",
    "pm2-reload:prod": "pm2 reload ecosystem.config.js --env prod",
    "pm2-start:staging": "pm2-runtime start pm2-staging.json",
    "pm2-reload:staging": "pm2-runtime reload pm2-staging.json",
    "prepare": "husky install"
  },
